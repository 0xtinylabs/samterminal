{
  "name": "@samterminal/plugin-ai",
  "version": "1.0.0",
  "description": "AI/LLM plugin for SamTerminal - supports OpenAI, Anthropic, and more",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./actions": {
      "import": "./dist/actions/index.js",
      "types": "./dist/actions/index.d.ts"
    },
    "./providers": {
      "import": "./dist/providers/index.js",
      "types": "./dist/providers/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:cov": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "openai": "^4.77.0"
  },
  "peerDependencies": {
    "@samterminal/core": "*"
  },
  "devDependencies": {
    "@samterminal/core": "*",
    "@samterminal/tsconfig": "*",
    "@types/jest": "^30.0.0",
    "jest": "^30.0.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.0.0",
    "typescript": "^5.7.3"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "samterminal",
    "plugin",
    "ai",
    "llm",
    "openai",
    "anthropic",
    "claude",
    "gpt"
  ],
  "author": "SamTerminal Contributors",
  "license": "MIT",
  "homepage": "https://github.com/samterminal/samterminal/tree/main/packages/plugins/ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/samterminal/samterminal.git",
    "directory": "packages/plugins/ai"
  },
  "bugs": {
    "url": "https://github.com/samterminal/samterminal/issues"
  }
}
