{
  "name": "@samterminal/testing-utils",
  "version": "1.0.0",
  "description": "SamTerminal Testing Utilities - Mocks, fixtures, and factories for testing",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./mocks": {
      "import": "./dist/mocks/index.js",
      "types": "./dist/mocks/index.d.ts"
    },
    "./fixtures": {
      "import": "./dist/fixtures/index.js",
      "types": "./dist/fixtures/index.d.ts"
    },
    "./factories": {
      "import": "./dist/factories/index.js",
      "types": "./dist/factories/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist node_modules",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "samterminal",
    "testing",
    "mocks",
    "fixtures"
  ],
  "author": "SamTerminal Contributors",
  "license": "MIT",
  "homepage": "https://github.com/samterminal/samterminal/tree/main/packages/testing-utils",
  "repository": {
    "type": "git",
    "url": "https://github.com/samterminal/samterminal.git",
    "directory": "packages/testing-utils"
  },
  "bugs": {
    "url": "https://github.com/samterminal/samterminal/issues"
  },
  "devDependencies": {
    "@samterminal/tsconfig": "*",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "tsup": "^8.0.0",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "jest": "^30.0.0"
  },
  "peerDependenciesMeta": {
    "jest": {
      "optional": false
    }
  }
}
